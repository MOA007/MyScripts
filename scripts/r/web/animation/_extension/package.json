{
  "name": "my-animation-extension",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.32.0"
  },
  "repository": {
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yo.runSelection",
        "title": "VideoTool: Run Selection"
      },
      {
        "command": "yo.exportAudio",
        "title": "VideoTool: Export Audio Only"
      },
      {
        "command": "yo.startRecording",
        "title": "VideoTool: Start Recording"
      },
      {
        "command": "yo.stopRecording",
        "title": "VideoTool: Stop Recording"
      },
      {
        "command": "yo.collectNoiseProfile",
        "title": "VideoTool: Collect Noise Profile"
      },
      {
        "command": "yo.openFileUnderCursor",
        "title": "VideoTool: Open File Under Cursor"
      },
      {
        "command": "yo.insertAllClipsInFolder",
        "title": "VideoTool: Insert All Clips In Folder"
      },
      {
        "command": "yo.removeUnusedRecordings",
        "title": "VideoTool: Remove Unused Recordings"
      },
      {
        "command": "yo.showStats",
        "title": "VideoTool: Show Stats"
      },
      {
        "command": "yo.createSlide",
        "title": "VideoTool: Create Slide"
      }
    ],
    "keybindings": [
      {
        "command": "yo.runSelection",
        "key": "alt+enter"
      },
      {
        "command": "yo.exportAudio",
        "key": "ctrl+alt+enter"
      },
      {
        "command": "yo.startRecording",
        "key": "f7"
      },
      {
        "command": "yo.stopRecording",
        "key": "f8"
      },
      {
        "command": "yo.collectNoiseProfile",
        "key": "f9"
      },
      {
        "command": "yo.openFileUnderCursor",
        "key": "f4"
      }
    ],
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "tslint -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "tslint": "^5.19.0",
    "typescript": "^3.8.3",
    "@types/vscode": "^1.32.0"
  }
}